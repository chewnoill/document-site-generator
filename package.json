{
  "name": "document-site-builder",
  "version": "1.0.25",
  "author": "Will Cohen <chewnoill@gmail.com>",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && yarn build && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "build": "tsc && cp -R src/main/components lib/main/ && cp src/reveal.js lib/",
    "dev": "tsc && webpack serve --config lib/webpack/dev.config.js",
    "debug": "yarn build && ./bin/run",
    "build:osx": "oclif-dev pack:macos",
    "build:deb": "oclif-dev pack:deb",
    "build:static": "yarn build && webpack --config lib/webpack/main-prod.config.js",
    "build:docs": "yarn dev:run build docs",
    "format": "prettier -w src/*"
  },
  "bin": {
    "docs": "./bin/run"
  },
  "bugs": "https://github.com/chewnoill/document-site-builder/issues",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.5.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mapbox/rehype-prism": "0.5.0",
    "@mdx-js/loader": "^1.6.22",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/react": "^16",
    "@types/react-dom": "^16",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-prerender-plugin": "^0.1.0",
    "http-server": "^14.0.0",
    "markdown-loader": "^6.0.0",
    "mermaid": "^8.13.3",
    "next": "^12.0.4",
    "prerender-loader": "^1.3.0",
    "prismjs": "^1.25.0",
    "react": "^17",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^17",
    "react-stl-viewer": "^2.0.3",
    "remark-emoji": "^2",
    "reveal.js": "^4.2.1",
    "source-map": "^0.7.3",
    "style-loader": "^3.3.1",
    "three": "^0.135.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.61.0",
    "webpack-dev-server": "^4.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^16.11.6",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "prettier": "^2.4.1",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "ts-node": "^8.10.2",
    "webpack-cli": "^4.9.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/chewnoill/document-site-builder",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "document-site-builder",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.orchestrator.command"
    }
  },
  "repository": "chewnoill/document-site-builder",
  "types": "lib/index.d.ts"
}
