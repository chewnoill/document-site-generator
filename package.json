    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/loader": "^1.6.22",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "handlebars": "^4.7.7",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.0.0",
    "markdown-loader": "^6.0.0",
    "mdx-hydra": "^0.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "source-map": "^0.7.3",
    "tslib": "^1",
