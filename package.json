  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^16.11.6",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
